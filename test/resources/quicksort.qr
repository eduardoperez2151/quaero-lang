@
    function quicksort(list){
        if (list==[]) return [];
        else {
            piv=list[0];
            l=[y for y <- list, y <= piv];
            r=[y for y <- list, y > piv]
            return quicksort(l) ++ quicksort(r);
        }
    }
    main:{
        a=[3,5,7,1,2,87,78,9,4];
        b=quickSort(a);
    }
@