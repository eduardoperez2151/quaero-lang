    function quicksort(list){
        if (list==[]) return [];
        else {
            x=list[0];

            xs=slice(list,1,#list);
            l=[y for y <- xs, y <= x];
            print(xs);
            r=[y for y <- xs, y > x];
            return quicksort(l) ++[x]++ quicksort(r);
        }
    }
    main:{
        unSortedList=[3,5,7,1,2,87,78,9,4];
        sortedList=quicksort(unSortedList);
    }
